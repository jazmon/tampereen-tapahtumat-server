{
  "name": "tampereen-tapahtumat-server",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "npm": ">=3.10.9",
    "node": ">=7.2.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "build": "babel --ignore *.spec.js -d ./build ./src -s",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha src/**/*.spec.js",
    "clean": "rimraf build && mkdir build",
    "test:watch": "chokidar \"src/**/*.js\" \"test/**/*.js\" -c \"npm test\"",
    "lint": "node_modules/.bin/eslint src",
    "lint:fix": "node_modules/.bin/eslint src --fix",
    "heroku-postbuild": "npm build"
  },
  "description": "a server for tampereen-tapahtumat",
  "main": "index.js",
  "author": "Atte Huhtakangas <atte.huhtakangas@gmail.com>",
  "license": "MIT",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "@google/maps": "^0.3.0",
    "babel-cli": "^6.18.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-lodash": "^3.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^0.0.9",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.18.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-timeout": "^1.8.0",
    "cross-env": "^3.1.3",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-graphql": "^0.6.1",
    "express-slash": "^2.0.1",
    "graphql": "^0.8.2",
    "helmet": "^3.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.2",
    "moment": "^2.17.0",
    "morgan": "^1.7.0",
    "newrelic": "^1.34.0",
    "response-time": "^2.3.2",
    "rimraf": "^2.5.4",
    "view-helpers": "^0.1.5"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.28.2",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "fetch-mock": "^5.5.0",
    "flow-bin": "^0.36.0",
    "isparta": "^4.0.0",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "sinon": "^1.17.6"
  }
}
